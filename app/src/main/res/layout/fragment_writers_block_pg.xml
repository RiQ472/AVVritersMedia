<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".WritersBlockPg">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="89dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="642dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <Button
            android:id="@+id/button_WritersBlock"
            android:layout_width="190dp"
            android:layout_height="89dp"
            android:layout_alignParentRight="true"
            android:layout_marginStart="279dp"
            android:layout_marginRight="21dp"
            android:layout_marginBottom="642dp"
            android:background="@drawable/sharp_add_2_24"
            android:fontFamily="@font/aladin"
            android:text="Writers Block"
            android:textColor="@color/Magenta"
            android:textSize="18dp" />


        <ImageButton
            android:layout_height="5dp"
            android:layout_width="75dp"
            android:id="@+id/button_WritersBlock_line"
            android:layout_alignParentEnd="true"
            android:layout_alignBottom="@+id/button_WritersBlock"
            android:background="@color/Magenta"
            android:layout_marginRight="25dp"
            />

        <Button
            android:id="@+id/button_Inspiration"
            android:layout_width="158dp"
            android:layout_height="90dp"
            android:layout_centerInParent="true"
            android:background="@drawable/sharp_add_2_24"
            android:backgroundTint="@color/Magenta"
            android:fontFamily="@font/aladin"
            android:text="Inspiration"
            android:textAlignment="center"
            android:textColor="@color/Magenta"
            android:textSize="21sp" />
        <ImageButton
            android:id="@+id/button_Inspo_line"
            android:layout_width="75dp"
            android:layout_height="5dp"
            android:layout_centerInParent="true"
            android:layout_alignBottom="@+id/button_WritersBlock"
            android:background="@color/Magenta"
            />
        <Button
            android:id="@+id/button_Idea"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="3dp"
            android:layout_marginEnd="270dp"
            android:layout_marginBottom="642dp"
            android:background="@drawable/sharp_add_2_24"
            android:fontFamily="@font/aladin"
            android:text="Idea"
            android:textAlignment="center"
            android:textColor="@color/Magenta"
            android:textSize="24sp" />
        <ImageButton
            android:id="@+id/button_Idea_line"
            android:layout_height="5dp"
            android:layout_width="75dp"
            android:layout_alignParentStart="true"
            android:layout_alignBottom="@+id/button_WritersBlock"
            android:background="@color/Magenta"
            android:layout_marginStart="20dp"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="89dp"
        android:layout_marginTop="638dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/button_AddIdea"
            android:layout_width="85dp"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="3dp"
            android:background="@drawable/sharp_add_usermainpg_2_24"
            android:src="@drawable/sharp_add_usermainpg_2_24"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:src="@drawable/sharp_add_usermainpg_2_24" />

        <Button
            android:id="@+id/button_MyIdeas"
            android:layout_width="166dp"
            android:layout_height="70dp"
            android:layout_centerInParent="true"
            android:background="@drawable/sharp_add_2_24"
            android:fontFamily="@font/aladin"
            android:text="My Ideas"
            android:textColor="@color/Magenta"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button_Rooms"
            android:layout_width="122dp"
            android:layout_height="84dp"
            android:layout_alignParentRight="true"
            android:background="@drawable/sharp_add_2_24"
            android:fontFamily="@font/aladin"
            android:text="Rooms"
            android:textColor="@color/Magenta"
            android:textSize="20sp" />
    </RelativeLayout>


    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="385dp"
        android:layout_height="273dp"
        android:background="@color/grey"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.257">


    </androidx.recyclerview.widget.RecyclerView>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="163dp"
        android:layout_height="57dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="137dp"
        android:layout_marginEnd="156dp"
        android:layout_marginBottom="537dp"
        android:text="user95"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="359dp"
        android:layout_height="63dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="225dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="443dp"
        android:text="Look at references to your idea it may bring the spark back!"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="63dp"
        android:layout_height="57dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="381dp"
        android:layout_marginEnd="282dp"
        android:layout_marginBottom="293dp"
        android:src="@drawable/baseline_comment_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="308dp"
        android:layout_marginTop="381dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="278dp"
        android:src="@mipmap/ic_tree"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="62dp"
        android:layout_height="56dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="382dp"
        android:layout_marginEnd="345dp"
        android:layout_marginBottom="293dp"
        android:src="@mipmap/ic_like_round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="121dp"
        android:layout_marginEnd="307dp"
        android:layout_marginBottom="524dp"
        android:src="@drawable/baseline_circle_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.458" />
</androidx.constraintlayout.widget.ConstraintLayout>