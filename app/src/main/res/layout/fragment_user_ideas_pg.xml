<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    android:tag="tag_user_ideas_pg"
    tools:context=".UserIdeasPg">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="404dp"
        android:layout_height="576dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="149dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="6dp"
        android:id="@+id/idea_list_layout_view"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <TextView
            android:id="@+id/textView1"
            android:layout_width="383dp"
            android:layout_height="66dp"
            android:background="@color/grey"
            android:backgroundTint="@color/grey"
            android:layout_centerHorizontal="true"
            android:layout_alignParentTop="true" />
        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="43dp"
            android:layout_height="39dp"
            android:layout_marginStart="352dp"
            android:layout_marginTop="5dp"
            android:src="@android:drawable/ic_menu_edit"
            android:layout_alignRight="@+id/textView1"
            android:layout_alignTop="@+id/textView1"/>

    </RelativeLayout>

    <TextView
        android:id="@+id/textViewPgTittle"
        android:layout_width="397dp"
        android:layout_height="67dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="617dp"
        android:fontFamily="sans-serif"
        android:inputType="text"
        android:text="My Ideas"
        android:textAlignment="center"
        android:textColor="@color/Magenta"
        android:textSize="48sp"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/buttonBackMyIdeasPg"
        android:layout_width="60dp"
        android:layout_height="58dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="344dp"
        android:layout_marginBottom="663dp"
        android:background="@color/black"
        android:scaleType="fitCenter"
        android:src="@drawable/baseline_arrow_back_ios_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />




</androidx.constraintlayout.widget.ConstraintLayout>