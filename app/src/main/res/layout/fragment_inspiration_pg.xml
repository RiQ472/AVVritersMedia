<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".InspirationPg">


    <ImageButton
        android:id="@+id/imageButton_menu"
        android:layout_width="65dp"
        android:layout_height="76dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="342dp"
        android:layout_marginBottom="653dp"

        android:background="@mipmap/ic_top_menu_black_megenta_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="89dp"
        android:layout_marginTop="638dp"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/button_WritersBlock"
            android:layout_width="190dp"
            android:layout_height="89dp"
            android:layout_alignParentRight="true"
            android:layout_marginStart="279dp"
            android:layout_marginRight="21dp"
            android:layout_marginBottom="642dp"
            android:background="@drawable/sharp_add_2_24"
            android:fontFamily="@font/aladin"
            android:text="Writers Block"
            android:textColor="@color/Magenta"
            android:textSize="18dp" />


        <ImageButton
            android:id="@+id/button_WritersBlock_line"
            android:layout_width="75dp"
            android:layout_height="5dp"
            android:layout_alignBottom="@+id/button_WritersBlock"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="25dp"
            android:background="@color/Magenta" />

        <Button
            android:id="@+id/button_Inspiration"
            android:layout_width="158dp"
            android:layout_height="90dp"
            android:layout_centerInParent="true"
            android:background="@drawable/sharp_add_2_24"
            android:backgroundTint="@color/Magenta"
            android:fontFamily="@font/aladin"
            android:text="Inspiration"
            android:textAlignment="center"
            android:textColor="@color/Magenta"
            android:textSize="21sp" />

        <ImageButton
            android:id="@+id/button_Inspo_line"
            android:layout_width="75dp"
            android:layout_height="5dp"
            android:layout_alignBottom="@+id/button_WritersBlock"
            android:layout_centerInParent="true"
            android:background="@color/Magenta" />

        <Button
            android:id="@+id/button_Idea"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="3dp"
            android:layout_marginEnd="270dp"
            android:layout_marginBottom="642dp"
            android:background="@drawable/sharp_add_2_24"
            android:fontFamily="@font/aladin"
            android:text="Idea"
            android:textAlignment="center"
            android:textColor="@color/Magenta"
            android:textSize="24sp" />

        <ImageButton
            android:id="@+id/button_Idea_line"
            android:layout_width="75dp"
            android:layout_height="5dp"
            android:layout_alignBottom="@+id/button_WritersBlock"
            android:layout_alignParentStart="true"
            android:layout_marginStart="20dp"
            android:background="@color/Magenta" />


    </RelativeLayout>


    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="385dp"
        android:layout_height="273dp"
        android:background="@color/grey"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.257">


    </androidx.recyclerview.widget.RecyclerView>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="163dp"
        android:layout_height="57dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="137dp"
        android:layout_marginEnd="156dp"
        android:layout_marginBottom="537dp"
        android:text="user95"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="359dp"
        android:layout_height="63dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="225dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="443dp"
        android:text="Look at references to your idea it may bring the spark back!"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="63dp"
        android:layout_height="57dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="381dp"
        android:layout_marginEnd="282dp"
        android:layout_marginBottom="293dp"
        android:src="@drawable/baseline_comment_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="308dp"
        android:layout_marginTop="381dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="278dp"
        android:src="@mipmap/ic_tree"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="62dp"
        android:layout_height="56dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="382dp"
        android:layout_marginEnd="345dp"
        android:layout_marginBottom="293dp"
        android:src="@mipmap/ic_like_round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="121dp"
        android:layout_marginEnd="307dp"
        android:layout_marginBottom="524dp"
        android:src="@drawable/baseline_circle_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.458" />

    <RelativeLayout
        android:layout_width="214dp"
        android:layout_height="196dp"
        android:layout_marginEnd="189dp"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="525dp"
        android:background="@color/black"
        android:id="@+id/relative_layout_menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:visibility="gone"
        app:layout_constraintTop_toTopOf="parent">


        <TextView
            android:id="@+id/textView_profile"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:background="@color/grey"
            android:paddingTop="20dp"
            android:text="Profile"
            android:paddingStart="40dp"
            android:textAlignment="textStart"
            android:textColor="@color/black"
            android:layout_marginEnd="3dp"
            android:textSize="24sp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/textViewMyRooms"
            android:layout_width="match_parent"
            android:layout_height="63dp"
            android:layout_below="@+id/textView_profile"
            android:layout_marginTop="3dp"
            android:background="@color/grey"
            android:paddingStart="40dp"
            android:textAlignment="textStart"
            android:outlineAmbientShadowColor="@color/black"
            android:paddingTop="20dp"
            android:text="My Rooms"
            android:layout_marginEnd="3dp"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:textStyle="bold" />
        <ImageButton
            android:id="@+id/imageButton_MyRooms"
            android:layout_width="match_parent"
            android:layout_height="69dp"
            android:layout_marginTop="3dp"
            android:layout_below="@+id/textView_profile"
            android:background="@drawable/sharp_add_2_24" />
        <ImageButton
            android:id="@+id/imageButton_profile"
            android:layout_width="match_parent"
            android:layout_height="53dp"
            android:background="@drawable/sharp_add_2_24" />
        <TextView
            android:id="@+id/textViewMyideas"
            android:layout_width="match_parent"
            android:layout_height="69dp"
            android:layout_below="@+id/textViewMyRooms"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="3dp"
            android:background="@color/grey"
            android:outlineAmbientShadowColor="@color/black"
            android:paddingTop="20dp"
            android:paddingStart="40dp"
            android:textAlignment="textStart"
            android:text="My Ideas"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:textStyle="bold" />
        <ImageButton
            android:id="@+id/imageButton_MyIdeas"
            android:layout_width="match_parent"
            android:layout_height="69dp"
            android:layout_marginTop="3dp"
            android:layout_below="@+id/textViewMyRooms"
            android:background="@drawable/sharp_add_2_24" />
        <ImageButton
            android:id="@+id/button_AddIdea"
            android:layout_width="54dp"
            android:layout_height="48dp"
            android:layout_below="@+id/textViewMyRooms"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_marginLeft="160dp"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="0dp"
            android:background="@drawable/sharp_add_idea_black_background" />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>